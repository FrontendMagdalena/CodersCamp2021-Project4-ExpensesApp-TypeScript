{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "rules": {
    "semi": ["error", "always"],
    "react/react-in-jsx-scope": "off",
    "no-undef": "off",
    "react/prop-types": "off"

  },
  "parserOptions": {
    "sourceType": "module"
  }
}
